Dates[0];\r\n        return wordDate;\r\n    }\r\n\r\n    function formatBody(body) {\r\n        return body.replace(/\\n/g, \"\\n\\n\");\r\n    }\r\n    \r\n    return(\r\n        <div className = \"post-preview\">\r\n            <h3>{props.title}</h3>\r\n            <p className = \"date\">{fixDate(props.createdAt)}</p>\r\n            <br />\r\n            <p className = \"body\">{formatBody(props.body)}</p>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default PostPreview;","C:\\Users\\Hunter\\Documents\\Code\\TheGoodStuff\\client\\src\\pages\\Article\\index.js",["115","116","117","118","119","120","121","122","123","124","125"],"C:\\Users\\Hunter\\Documents\\Code\\TheGoodStuff\\client\\src\\components\\Aside\\index.js",["126","127","128","129","130","131"],"import {useEffect, useState} from \"react\";\r\nimport \"./style.css\";\r\n\r\nfunction Aside() {\r\n\r\n    const [visible, setVisible] = useState(true);\r\n    const [yPosition, setYPosition] = useState(0);\r\n   \r\n\r\n    return(\r\n        <aside>\r\n            <div id = \"info\">\r\n                <p><em>\"Tardyblom\"</em><br />What does it mean? Nothing at all!</p>\r\n                <hr />\r\n                <p>This site is one man's attempt to be the next IGN.</p>\r\n                <br />\r\n                <p>You can expect anything from standard reviews to ruminations on different hobbies. \r\n                    Additionally, there'll be the occasional article completely out of left field.</p>\r\n            </div>\r\n            <div id = \"icon-box\">\r\n                <a href = \"https://twitter.com/tardyblom\" target = \"__blank\">\r\n                   <img className = \"icon\" src = \"/images/twitter-icon.png\" />\r\n                </a>\r\n            </div>\r\n        </aside>\r\n    )\r\n}\r\n\r\nexport default Aside;","C:\\Users\\Hunter\\Documents\\Code\\TheGoodStuff\\client\\src\\components\\Dropdown\\index.js",["132","133","134","135"],"import {useEffect, useState} from \"react\"